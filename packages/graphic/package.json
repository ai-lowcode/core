{
  "name": "@zero-dim/graphic",
  "type": "module",
  "version": "1.0.0",
  "description": "@zero-dim/graphic for ZeroDim NoCode",
  "homepage": "https://github.com/zero-dim/core/#readme",
  "repository": {
    "type": "http",
    "url": "https://github.com/zero-dim/core"
  },
  "bugs": "https://github.com/zero-dim/core/issues",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/zero-dim-graphic.js",
      "require": "./dist/zero-dim-graphic.js"
    },
    "./css": {
      "import": "./dist/style.css"
    }
  },
  "main": "./dist/zero-dim-graphic.js",
