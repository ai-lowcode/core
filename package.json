{
  "name": "ailowcode",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.4.0",
  "description": "a ai lowcode project",
  "author": "A.L",
  "license": "MIT",
  "keywords": [],
  "engines": {
    "node": ">=18.20.0",
    "npm": ">=9.4.0"
  },
  "scripts": {
    "dev": "pnpm --filter \"ai-lowcode-server\" \"dev\"",
    "build": "node ./scripts/build.js",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && pnpm release:only",
    "release:only": "changeset publish --registry=https://registry.npmjs.com/",
    "test": "vitest",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "stylelint": "stylelint \"**/*.{html,vue,css,scss}\"",
    "prepare": "husky install",
    "cmt": "git add . && git-cz",
    "clean": "pnpm turbo clean && rimraf node_modules",
    "clean:dist": "pnpm turbo clean:dist && rimraf dist",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "devDependencies": {
    "@al-config/lint": "workspace:^",
    "@al-config/scripts": "workspace:^",
    "@al-config/tsconfig": "workspace:^",
    "@antfu/eslint-config": "^2.24.0",
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.7",
    "@commitlint/cli": "^19.3.0",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^5.1.1",
    "eslint": "^9.8.0",
    "git-cz": "^4.9.0",
    "husky": "^9.1.3",
    "lint-staged": "^15.2.7",
    "rimraf": "^5.0.7",
    "stylelint": "^16.8.0",
    "ts-node": "^10.9.2",
    "turbo": "^2.0.9",
    "typescript": "^5.5.4",
    "vite": "^5.3.5",
    "vitepress": "^1.3.1",
    "vitest": "^2.0.4"
  },
  "lint-staged": {
    "**/**/*.{vue,js,jsx,ts,tsx}": "pnpm lint:fix"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
