    "git-cz": "^4.9.0",
    "husky": "^9.1.3",
    "lint-staged": "^15.2.7",
    "ts-node": "^10.9.2",
    "turbo": "^2.0.9",
    "typescript": "^5.5.4",
    "vite": "^5.3.5"
  },
  "lint-staged": {
    "**/**/*.{js,jsx,ts,tsx}": "pnpm lint:fix",
