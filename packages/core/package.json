{
  "name": "@ai-lowcode/core",
  "type": "module",
  "version": "1.0.0",
  "description": "element-plus for ai-lowcode",
  "homepage": "https://github.com/ai-lowcode/core/#readme",
  "repository": {
    "type": "http",
    "url": "https://github.com/ai-lowcode/core"
  },
  "bugs": "https://github.com/ai-lowcode/core/issues",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ai-lowcode-core.js",
      "require": "./dist/ai-lowcode-core.js"
    },
    "./css": {
      "import": "./dist/style.css"
    }
  },
  "main": "./dist/ai-lowcode-core.js",
  "module": "./dist/ai-lowcode-core.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "build:all": "vite build && pnpm build:dts",
    "build:dts": "rimraf temp && tsc -p tsconfig.declaration.json && rollup-dts",
    "clean": "rimraf node_modules",
    "clean:mac": "rimraf node_modules",
    "clean:dist": "rimraf dist"
  },
  "dependencies": {
    "@ai-lowcode/designer": "workspace:^",
    "@ai-lowcode/utils": "workspace:^",
    "vue": "^3.4.34"
  },
  "devDependencies": {
    "@al-config/lint": "workspace:^",
    "@al-config/tsconfig": "workspace:^",
    "@al-config/vite": "workspace:^",
    "@antfu/eslint-config": "^2.24.0",
    "@iconify/json": "^2.2.233",
    "@iconify/vue": "^4.1.2",
    "@tomjs/vite-plugin-monaco-editor": "^1.1.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^9.8.0",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
    "vite": "^5.3.5",
    "vite-plugin-monaco-editor": "^1.1.0",
    "vitest": "^2.0.4"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
