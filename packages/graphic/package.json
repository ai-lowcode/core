    "@al-config/tsconfig": "workspace:^",
    "@al-config/vite": "workspace:^",
    "@iconify/json": "^2.2.233",
    "@iconify/vue": "^4.1.2",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.8.0",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.10",
    "vite": "^5.4.3",
    "vitest": "^2.0.4",
