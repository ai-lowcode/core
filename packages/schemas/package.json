{
  "name": "@zero-dim/schemas-component-adapter",
  "type": "module",
  "version": "1.0.0",
  "description": "@zero-dim/schemas-component-adapter for ZeroDim NoCode",
  "homepage": "https://github.com/zero-dim/core/#readme",
  "repository": {
    "type": "http",
    "url": "https://github.com/zero-dim/core"
  },
  "bugs": "https://github.com/zero-dim/core/issues",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/zero-dim-schemas-component-adapter.js",
      "require": "./dist/zero-dim-schemas-component-adapter.js"
    },
    "./css": {
      "import": "./dist/style.css"
    }
  },
  "main": "./dist/zero-dim-schemas-component-adapter.js",
  "module": "./dist/zero-dim-schemas-component-adapter.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "serve": "vite build --watch",
    "build": "vite build",
    "build:all": "vite build && pnpm build:dts",
    "build:dts": "rimraf temp && tsc -p tsconfig.declaration.json && rollup-dts",
    "clean": "rimraf node_modules",
    "clean:dist": "rimraf dist"
  },
  "peerDependencies": {
    "vue": "^3.5.3"
  },
  "dependencies": {
    "@zero-dim/component-adapter": "workspace:^",
    "@zero-dim/core": "workspace:^",
    "@zero-dim/request": "workspace:^",
    "@zero-dim/styles": "workspace:^",
    "@zero-dim/utils": "workspace:^"
  },
  "devDependencies": {
    "@al-config/lint": "workspace:^",
    "@al-config/tsconfig": "workspace:^",
    "@al-config/vite": "workspace:^",
    "@antfu/eslint-config": "^2.24.0",
    "@iconify/json": "^2.2.233",
    "@iconify/vue": "^4.1.2",
    "@zero-dim/types": "workspace:^",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.8.0",
    "postcss": "^8.4.45",
    "tailwindcss": "^3.4.10",
    "vite": "^5.4.3",
    "vitest": "^2.0.4",
    "vue": "^3.5.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
