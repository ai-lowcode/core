{
  "name": "ailowcode",
  "version": "1.0.0",
  "private": true,
  "description": "a ai lowcode project",
  "author": "A.L",
  "license": "MIT",
  "keywords": [],
  "engines": {
    "node": ">=18.20.0",
    "npm": ">=9.4.0"
  },
  "scripts": {
    "lint": "eslint . --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "prepare": "husky install",
    "cmt": "git add . && git-cz"
  },
  "devDependencies": {
    "@al-config/lint": "workspace:^",
    "@antfu/eslint-config": "^2.24.0",
    "@commitlint/cli": "^19.3.0",
    "eslint": "^9.8.0",
    "git-cz": "^4.9.0",
    "husky": "^9.1.3",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.3"
  },
  "lint-staged": {
    "**/**/*.{js,jsx,ts,tsx}": "pnpm lint:fix",
    "**/**/*.{vue,js,ts,jsx,tsx}": "pnpm prettier:fix"
  },
  "config": {
    "commitizen": {
      "path": "git-cz"
    }
  }
}
