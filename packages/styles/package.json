    "@al-config/vite": "workspace:^",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.40",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.7",
    "vite": "^5.3.5",
    "vitest": "^2.0.4"
